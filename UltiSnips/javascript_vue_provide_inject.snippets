extends javascript,vue

snippet provide "vue provide" b
  // provide should be placed before prop and data
  provide() {
    return {
      name: 'value'
      [symbol]: 'value'
      // TODO
    }
  },

endsnippet


snippet inject "vue inject with array" b
  // inject should be placed before prop and data
  inject: [ 'name' ],

endsnippet

snippet injectsym "vue inject with symbol" b
  // inject should be placed before prop and data
  inject: {
    symbol
  },

endsnippet

snippet injectsym "vue inject with symbol and alias" b
  // inject should be placed before prop and data
  inject: {
    aliasName: {
      from: symbol
    }
  },

endsnippet

snippet injectsymdef "vue inject with symbol and default value" b
  // inject should be placed before prop and data
  inject: {
    aliasName: {
      from: symbol
      default: 'default'
    }
  },

endsnippet

snippet injectsymdefobj "vue inject with symbol and default object" b
